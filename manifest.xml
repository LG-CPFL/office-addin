  <SupportUrl DefaultValue="https://learn.microsoft.com/en-us/office/dev/add-ins/overview/office-add-ins"/>
  <AppDomains>
    <AppDomain>https://raw.githubusercontent.com/LG-CPFL/office-addin/main</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Document"/>
  </Hosts>

  <!-- these parts are required-->
  <DefaultSettings>
    <SourceLocation DefaultValue="https://lg-cpfl.github.io/office-addin/src/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>

