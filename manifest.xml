<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="TaskPaneApp"> 
  <!-- these are required sections-->
  <Id>F3E9A0E2-00F9-4DC3-A535-3A988F3A2AE6</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>CPFL</ProviderName>
  <DefaultLocale>en-AU</DefaultLocale>
  <DisplayName DefaultValue="CPFL"/>
  <Description DefaultValue="A simple Taskpane add-in for CPFL."/>

  <!-- these bits are optional-->
